{
    "framework": "vite",
    "buildCommand": "pnpm run build",
    "outputDirectory": "dist",
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/admin",
            "destination": "/admin.html"
        },
        {
            "source": "/((?!manifest.json|sw.js|js/|css/|img/).*)",
            "destination": "/index.html"
        }
    ]
}