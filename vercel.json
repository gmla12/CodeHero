{
    "framework": "vite",
    "buildCommand": "pnpm run build",
    "outputDirectory": "dist",
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/admin",
            "destination": "/admin.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}